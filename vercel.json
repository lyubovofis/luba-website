{
  "rewrites": [
    {
      "source": "/crm",
      "destination": "/crm/login.html"
    },
    {
      "source": "/crm/",
      "destination": "/crm/index.html"
    },
    {
      "source": "/quiz",
      "destination": "/quiz-test/index.html"
    },
    {
      "source": "/test",
      "destination": "/quiz-test/index.html"
    }
  ],
  "headers": [
    {
      "source": "/crm/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
